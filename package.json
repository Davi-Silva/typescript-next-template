{
  "name": "typescript-next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "jest",
    "build": "next build",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "globby": "^11.0.1",
    "immutability-helper": "^3.1.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.20",
    "next": "10.0.6",
    "next-images": "^1.4.1",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^5.0.0",
    "portrait-load": "1.0.23",
    "prettier": "^2.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^3.11.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "slugify": "^1.4.6",
    "styled-components": "^5.1.1",
    "unique-names-generator": "^4.3.1"
